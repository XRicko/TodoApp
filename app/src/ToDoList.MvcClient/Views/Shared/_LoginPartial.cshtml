@inject IStringLocalizer<General> Localizer

<ul class="navbar-nav">
@if (Context.Request.Cookies["accessToken"] is not null)
{
    <li class="nav-item">
        <form  class="form-inline" asp-controller="User" asp-action="Logout">
            <button  type="submit" class="nav-link btn btn-link text-dark">@Localizer["Log Out"]</button>
        </form>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-controller="User" asp-action="Register">@Localizer["Register"]</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-controller="User" asp-action="Login">@Localizer["Log In"]</a>
    </li>
}
</ul>
